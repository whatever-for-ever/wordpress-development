{
	"name": "an-whatever",
	"version": "1.0.0",
	"description": "A plugin to show your skills.",
	"author": "Whatever Forever",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"prettier": "@wordpress/prettier-config",
	"scripts": {
		"build": "wp-scripts build --webpack-copy-php --webpack-src-dir=./_src/blocks --output-path=./blocks",
		"start": "wp-scripts start --webpack-copy-php --webpack-src-dir=./_src/blocks --output-path=./blocks",
