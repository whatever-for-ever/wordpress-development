		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"css": "html,body {min-height: calc(100vh - var(--wp-admin--admin-bar--height, 0px));}body > div.wp-site-blocks {padding-top: 0;padding-bottom:0;min-height: calc(100vh - var(--wp-admin--admin-bar--height, 0px));display: grid;grid-template-rows: auto 1fr auto;}",
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--base)"
